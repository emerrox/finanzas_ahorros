{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/*": ["./resources/js/*"]
      },
      "jsx": "react",
      "module": "ESNext",
      "target": "ESNext",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "isolatedModules": true,
      "allowSyntheticDefaultImports": true,
    },
    "include": ["resources/js/**/*"]
  }